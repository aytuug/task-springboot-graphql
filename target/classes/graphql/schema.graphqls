type Player {
    id: ID!
    firstName: String,
    lastName: String,
    position : Position
}

enum Position {
    PG,
    SG,
    SF,
    PF,
    C
}

type Query{
    findAllPlayers: [Player]!
}

type Mutation{
    savePlayer(firstName:String, lastName:String, position: Position): Player
    delete(id:ID) : Player
}